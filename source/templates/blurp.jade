extend default

block content
	.col-md-6
		h3 Departure
		select(id='departureSelect')
			each stop in items
				Option(value=stop.id) #{stop.display_name}
	.col-md-6
		h3 Arrival
		select(id='arrivalSelect')
			each stop in items
				Option(value=stop.id) #{stop.display_name}
	.col-md-6
		button(.btn-primary, on-click="submitStops") Submit
		